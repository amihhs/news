<view class="detail"><view class="detail-title">{{''+formData.title+''}}</view><view class="detial-hearder"><view class="detail-header-logo"><image src="{{formData.author.avatar}}" mode="aspectFill"></image></view><view class="detail-header-content"><view class="detail-header-content-title">{{''+formData.author.author_name+''}}</view><view class="detail-header-content-info"><text>{{formData.create_time}}</text><text>{{formData.browse_count+"浏览"}}</text><text>{{formData.thumbs_up_count+"赞"}}</text></view></view><button class="detail-header-button" type="default" data-event-opts="{{[['tap',[['follow',['$0'],['formData.author.id']]]]]}}" onTap="__e">{{formData.is_author_like?'取消关注':'关注'}}</button></view><view class="detail-content"><u-parse vue-id="78cf7796-1" content="{{formData.content}}" noData="{{noData}}" onVueInit="__l"></u-parse><view class="detail-comment"><view class="comment-title">最新评论</view><view class="comment-content" a:for="{{commentList}}" a:for-item="i" a:for-index="index" a:key="index"><comments-box vue-id="{{'78cf7796-2-'+index}}" item="{{i}}" data-event-opts="{{[['^reply',[['reply']]]]}}" onReply="__e" onVueInit="__l"></comments-box></view></view></view><view class="detail-bottom"><view data-event-opts="{{[['tap',[['openComment',['$event']]]]]}}" class="detail-bottom-input" onTap="__e"><text>客官请评论</text><uni-icons vue-id="78cf7796-3" type="compose" size="16" color="#f07373" onVueInit="__l"></uni-icons></view><view class="detail-bottom-icon"><view data-event-opts="{{[['tap',[['openDetialComments',['$0'],['formData._id']]]]]}}" class="detail-bottom-icon-box" onTap="__e"><uni-icons vue-id="78cf7796-4" type="chat" size="16" color="#f07373" onVueInit="__l"></uni-icons></view><view data-event-opts="{{[['tap',[['likeTap',['$0'],['formData._id']]]]]}}" class="detail-bottom-icon-box" onTap="__e"><uni-icons vue-id="78cf7796-5" type="{{formData.is_like?'heart-filled':'heart'}}" size="16" color="#f07373" onVueInit="__l"></uni-icons></view><view data-event-opts=